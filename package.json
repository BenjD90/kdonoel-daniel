{
  "name": "kdos-daniel-web",
  "private": true,
  "description": "Kdos daniel web app",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack",
    "watch": "webpack --watch",
    "watch:hmr": "webpack-dev-server",
    "translate": "webpack --config webpack/tranlate-standalone-config.js",
    "tslint": "tslint --project tsconfig.json --type-check --format msbuild 'app/*.ts' 'app/**/*.ts'  'app/**/**/*.ts'  'app/**/**/**/*.ts'  'app/**/**/**/**/*.ts'   'app/**/**/**/**/**/*.ts' || true",
    "htmllint": " htmllint app/*.html app/**/*.html app/**/**/*.html app/**/**/**/*.html   app/**/**/**/**/*.html || true",
    "lint": "npm run tslint && npm run htmllint",
    "test": "karma start ./karma/karma.conf.js --single-run",
    "test:once": "karma start ./karma/karma.conf.js --single-run",
    "tdd": "karma start ./karma/karma.conf.js --auto-watch",
    "build": "SET \"NODE_ENV=production\" && webpack",
    "package": "npm run build && tar cf build/%npm_package_name%-v%npm_package_version%.tar dist/*"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
